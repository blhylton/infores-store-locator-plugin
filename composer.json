{
  "name": "stellar_studios/philly_swirl_store_locator",
  "description": "WordPress plugin that enables the store locator for Philly Swirl",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Barry Hylton",
      "email": "bhylton@stellarstudios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BLHylton\\InfoResStoreLocator\\": "src",
      "BLHylton\\InfoResStoreLocator\\WordPress\\": "inc"
    }
